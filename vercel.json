{
  "version": 2,
  "builds": [
    {
      "src": "simple_vercel_app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "simple_vercel_app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "SECRET_KEY": "demo-key-2024"
  }
}